@{

}

@model List<Reservation>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
             <td>Reservation ID</td>
            <td>Name of Group</td>
            <td>Size of Group</td>
            <td>Email</td>
            <td>Phone</td>
            <!-- <td>Time</td>-->
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
        <tr>
            <td>@x.ReservationId</td>
            <td>@x.GroupName</td>
            <td>@x.GroupSize</td>
            <td>@x.Email</td>
            <td>@x.Phone</td>


            <td><a asp-controller="Home" asp-action="Edit" asp-route-appointmentid="@x.ReservationId">Edit</a></td>
            <td><a asp-controller="Home" asp-action="Delete">Delete</a></td>
        </tr>
        }
    </tbody>
</table>