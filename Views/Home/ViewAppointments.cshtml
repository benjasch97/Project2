@{
}


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <td>Date</td>
            <td>Name of Group</td>
            <td>Size of Group</td>
            <td>Email</td>
            <td>Phone</td>
            <td>Time</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
            {
                <tr>
                    <td>@x.AppointmentId</td>
                    <td>@x.Date</td>
                    <td>@x.NameOfGroup</td>
                    <td>@x.SizeOfGroup</td>
                    <td>@x.Email</td>
                    <td>@x.Phone</td>
                    <td>@x.Time</td>

                    <td><a asp-controller="Home" asp-action="Edit" asp-route-appointmentid="@x.AppointmentId">Edit</a></td>
                    <td><a asp-controller="Home" asp-action="Delete">Delete</a></td>
                </tr>
            }
    </tbody>
</table>